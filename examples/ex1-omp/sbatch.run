#!/bin/bash
#SBATCH -J omp_hello
#SBATCH -o omp_hello.out
#SBATCH -e omp_hello.err
#SBATCH -p seas_iacs
#SBATCH -t 0-00:30
#SBATCH -N 1
#SBATCH -c 6
#SBATCH --mem=4000

# Set up environment
source new-modules.sh
module load intel/15.0.0-fasrc01

# Run program
export OMP_NUM_THREADS=$SLURM_CPUS_PER_TASK
srun -c $SLURM_CPUS_PER_TASK ./omp_hello.x
